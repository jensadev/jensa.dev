{% block content %}
<ul role="list" class="tags-list">
    {% for tag in collections.tagList %}
      {% if tag and tag != "" %}
        {% set tagUrl %}/tagg/{{ tag | slug }}/{% endset %}
        <li>
            <a href="/{{ page.lang }}/{{ tagUrl | url }}">{{ tag | capitalize }}</a>
        </li>
      {% endif %}
    {% endfor %}
</ul>
{% endblock %}