{%- from '../nav/macro.njk' import nav -%}

<header class="site-head">
    <div class="site-head__inner container">
        {{ nav({
            siteName: props.siteName,
            active: props.active,
            items: props.items
        }) }}
        <div class="site-grid">
            <h1 class="site-grid__full">{{ props.title | linebreak }}</h1>
        </div>
    </div>
</header>