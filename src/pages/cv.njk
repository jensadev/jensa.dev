---
title: CV
eleventyExcludeFromCollections: true
permalink: /cv/
---

{% extends 'pages/base.njk' %}

{% set pagesCheck = page.filePathStem.includes('/pages/') %}

{% block header %}
{{
siteHead({
title: title,
siteName: meta.siteName,
active: page.url,
items: collections.pages,
tags: tags,
category: category
})
}}
{% endblock %}

{% block content %}
<style>
    .with-sidebar {
        display: flex;
        flex-wrap: wrap;
        gap: var(--step-0);
    }

    .with-sidebar>header {
        width: 100%;
    }

    .with-sidebar>header>h2 {
        font-size: var(--step-3);
    }

    .sidebar {
        flex-basis: 200px;
        flex-grow: 1;
    }

    .sidebar>div>h3 {
        margin-bottom: var(--step--4);
        font-size: var(--step-0);
    }

    .cv-list {
        padding-left: 0;
        list-style-type: none;
        margin-top: 0;
    }

    .sidebar>picture>img {
        object-fit: cover;
        object-position: top;
        width: 200px;
        height: 200px;
        border-radius: 100%;
    }

    .not-sidebar {
        /* ↓ Grow from nothing */
        flex-basis: 0;
        flex-grow: 999;
        /* ↓ Wrap when the elements are of equal width */
        min-width: 50%;
    }

    .erf>h3,
    .erf__lead {
        margin-bottom: var(--step--4);
    }

    @media print {
        .adr-link::after {
            content: " (" attr(href) ") ";
        }

        .site-head,
        footer,
        .anchor {
            display: none;
        }
    }
</style>
