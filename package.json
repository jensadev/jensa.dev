{
    "name": "jensa.xyz",
    "version": "2.0.0",
    "description": "Personal website of Jens Andreasson",
    "scripts": {
        "watch:sass": "sass  --no-source-map --watch src/sass:public/css",
        "watch:eleventy": "eleventy --serve",
        "watch:rollup": "rollup -c -w",
        "build:sass": "sass  --no-source-map src/sass:public/css",
        "build:eleventy": "eleventy",
        "build:rollup": "rollup -c",
        "postbuild": "postcss public/css/*.css -u autoprefixer cssnano -r --no-map",
        "start": "cross-env ELEVENTY_ENV=dev npm-run-all --parallel watch:*",
        "build": "cross-env ELEVENTY_ENV=prod npm-run-all --parallel build:*",
        "lint": "eslint --fix .",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jensnti/jensa.xyz.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/jensnti/jensa.xyz/issues"
    },
    "homepage": "https://github.com/jensnti/jensa.xyz",
    "dependencies": {
        "@11ty/eleventy": "^1.0.0",
        "@11ty/eleventy-img": "^1.0.0",
        "@11ty/eleventy-plugin-rss": "^1.1.2",
        "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
        "@rollup/plugin-commonjs": "^21.0.3",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "autoprefixer": "^10.4.2",
        "cross-env": "^7.0.3",
        "date-fns": "^2.28.0",
        "fast-glob": "^3.2.11",
        "html-minifier": "^4.0.0",
        "jsdom": "^19.0.0",
        "markdown-it": "^12.3.2",
        "markdown-it-attrs": "^4.1.3",
        "markdown-it-link-attributes": "^4.0.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.5",
        "postcss-cli": "^8.3.1",
        "rollup": "^2.70.1",
        "rollup-plugin-terser": "^7.0.2",
        "sass": "^1.49.0"
    },
    "devDependencies": {
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "modularscale-sass": "^3.0.10",
        "prettier": "^2.5.1"
    }
}
