{
  "name": "jensa.xyz",
  "version": "1.0.1",
  "description": "Personal website of Jens Andreasson",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/scss:public/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass --no-source-map src/scss:public/css",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass --parallel build:*",
    "postbuild": "postcss public/css/*.css -u autoprefixer cssnano -r"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jensnti/jensa.xyz.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jensnti/jensa.xyz/issues"
  },
  "homepage": "https://www.jensa.xyz",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.9.0",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@tbranyen/jsdom": "^13.0.0",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.6",
    "date-fns": "^2.22.1",
    "fast-glob": "^3.2.5",
    "html-minifier": "^4.0.0",
    "image-size": "^0.9.7",
    "markdown-it": "^10.0.0",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-link-attributes": "^3.0.0",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.2",
    "postcss-cli": "^8.3.1",
    "sass": "^1.34.1"
  },
  "devDependencies": {
    "modularscale-sass": "^3.0.10"
  }
}
