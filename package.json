{
    "name": "jensa.dev",
    "version": "3.0.0",
    "description": "Personal website of Jens Andreasson",
    "scripts": {
        "watch:sass": "sass  --no-source-map --watch src/sass:src/_includes/assets/css",
        "watch:eleventy": "eleventy --serve",
        "watch:rollup": "rollup -c -w",
        "build:sass": "sass  --no-source-map src/sass:src/_includes/assets/css",
        "build:eleventy": "eleventy",
        "build:rollup": "rollup -c",
        "start": "cross-env ELEVENTY_ENV=dev npm-run-all build:sass  --parallel watch:*",
        "build": "cross-env ELEVENTY_ENV=prod npm-run-all build:sass --parallel build:*",
        "lint": "eslint --fix .",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jensnti/jensa.dev"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/jensnti/jensa.dev/issues"
    },
    "homepage": "https://github.com/jensnti/jensa.dev",
    "dependencies": {
        "@11ty/eleventy": "^2.0.0-beta.1",
        "@11ty/eleventy-img": "^3.0.0",
        "@11ty/eleventy-plugin-rss": "^1.1.2",
        "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
        "@rollup/plugin-commonjs": "^21.0.3",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@sindresorhus/slugify": "^2.1.0",
        "@thedigitalman/eleventy-plugin-toc-a11y": "^2.1.0",
        "autoprefixer": "^10.4.2",
        "cross-env": "^7.0.3",
        "cssnano": "^5.1.12",
        "date-fns": "^2.28.0",
        "fast-glob": "^3.2.11",
        "html-minifier": "^4.0.0",
        "js-sha256": "^0.9.0",
        "jsdom": "^19.0.0",
        "markdown-it": "^12.3.2",
        "markdown-it-abbr": "^1.0.4",
        "markdown-it-anchor": "^8.6.4",
        "markdown-it-attrs": "^4.1.3",
        "markdown-it-link-attributes": "^4.0.0",
        "modern-css-reset": "^1.4.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.5",
        "postcss-cli": "^8.3.1",
        "prism-theme-vars": "^0.2.3",
        "rollup": "^2.70.1",
        "rollup-plugin-terser": "^7.0.2",
        "sass": "^1.49.0",
        "slugify": "^1.6.5"
    },
    "devDependencies": {
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "modularscale-sass": "^3.0.10",
        "prettier": "^2.5.1"
    }
}
